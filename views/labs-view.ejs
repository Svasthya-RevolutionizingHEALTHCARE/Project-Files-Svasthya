<!DOCTYPE html>
<html lang="en">
<head>
  <title>Lab Details</title>
   <!-- Google Fonts -->
   <link href="https://fonts.googleapis.com/css?family=Montserrat|Ubuntu" rel="stylesheet">

   <!-- CSS Stylesheets -->
   <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
   <link rel="stylesheet" href="css/styles_lab.css">
 
   <!-- Font Awesome -->
   <script defer src="https://use.fontawesome.com/releases/v5.0.7/js/all.js"></script>
 
   <!-- Bootstrap Scripts -->
   <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
   <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
   <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</head>
<body>
    <h2>Fetching the best results for you</h2>
    <section class='colored-section' id='title'>
    <div class="table-data">

    <table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Lab ID</th>
            
            <th scope="col">Lab Name</th>
            <th scope="col">Registration No.</th>
            <th scope="col">Test Required</th>
            <th scope="col">Address</th>
            <th scope="col">Fix Appointment</th>
            <!-- <th scope="col">Edit</th>
            <th scope="col">Delete</th> -->
        </tr>
    </thead>
    <tbody>
        
        <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
        <form action="/labs/lab-list/Appointment_lab" method="POST">
        <tr>
            
            <td scope="row"><input type="text"  name="labID"  value=<%=data.Lab_ID %> readonly></td>
            
            <td scope="row"><%=data.Lab_Name %></td>
            <td scope="row"><input type="text"  name="Reg_Number"  value=<%=data.Reg_Number %> readonly></td>
            <td scope="row"><%=data.Available_Tests %></td>
            <td scope="row"><%=data.Address %></td>
            <td scope="row"><button type="submit" class="btn btn-warning" name="button" >Book</button></td>
           
            
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </tbody>

    </form>
    </table>
</section>
    </div>
</body>
</html>